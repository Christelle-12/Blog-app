<div> 
 <img src="<%= @user.photo %>" alt ="individual user photo">
 <div>
   <h3> <%= @user.name%> </h3>
   <p>Number of posts: <%= @user.posts_counter || 0 %></p>
 </div>
 <div>
   <p><%= @user.bio%></p>
 </div>
 <div>
  <% @posts.each do |post|%>
   <h2><%= post.title%></h2>
   <p><%= post.text%></p>
  <%end%>
 </div>
 <button>
  <% @allposts %>
  See all posts
 </button>
</div>